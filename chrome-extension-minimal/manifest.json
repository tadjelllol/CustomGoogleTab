{
    "manifest_version": 5,
    "name": "Tadjell's Custom Tab",
    "version": "2.0",
    "description": "A custom new tab with random backgrounds and bookmarks.",
    "permissions": ["tabs","declarativeNetRequest"],
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
    
}
